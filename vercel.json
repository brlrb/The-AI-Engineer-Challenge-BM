{
    "version": 2,
    "builds": [
      { 
        "src": "frontend/package.json", 
        "use": "@vercel/next"
      },
      { 
        "src": "api/app.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb",
          "runtime": "python3.11"
        }
      }
    ],
    "routes": [
      { 
        "src": "/api/(.*)", 
        "dest": "api/app.py"
      }
    ]
  } 